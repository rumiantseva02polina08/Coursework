    def show_widget_2(self):
        self.widget_1.setVisible(False)
        self.widget_2.setVisible(True)

    def show_widget_3(self):
        self.pib = self.w2_textEdit_pib.toPlainText()
        self.number = self.w2_textEdit_number.toPlainText()
        self.city = self.w2_textEdit_city.toPlainText()
        self.email = self.w2_textEdit_email.toPlainText()
        if not (self.pib and self.number and self.city and self.email):
            QtWidgets.QMessageBox.warning(self, "Помилка", "Будь ласка, заповніть всі поля.")
        else:
            if not re.match(r'^[А-ЯІЇЄҐ\'’][а-яіїєґ\'’]+\s[А-ЯІЇЄҐ\'’][а-яіїєґ\'’]+\s[А-ЯІЇЄҐ\'’][а-яіїєґ\'’]+$', self.pib):
                QtWidgets.QMessageBox.warning(self, "Помилка",
                                              "Будь ласка, перевірте правильність введення ПІБ")
                return
            if not re.match(r'^380\d{9}$', self.number):
                QtWidgets.QMessageBox.warning(self, "Помилка",
                    "Будь ласка, введіть коректний номер телефону у форматі 380XXXXXXXXX")
                return
            if not re.match(r'^[\w\.-]+@[\w\.-]+\.[a-zA-Z]{2,}$', self.email):
                QtWidgets.QMessageBox.warning(self, "Помилка",
                                              "Будь ласка, перевірте правильність введення email")
                return
            if not re.match(r'^[А-ЯЇЄІҐ][а-яїєіґ]+$', self.city):
                QtWidgets.QMessageBox.warning(self, "Помилка",
                                              "Будь ласка, перевірте правильність введення міста")
                return

            self.widget_2.setVisible(False)
            self.widget_3.setVisible(True)
            try:
                with open('user_data_1.csv', 'a', newline='', encoding='utf-8') as file:
                    writer = csv.writer(file)
                    if file.tell() == 0:
                        writer.writerow(['ПІБ', 'Номер телефону', 'Email', 'Місто'])
                    writer.writerow([self.pib, self.number, self.email, self.city])
            except Exception as e:
                QtWidgets.QMessageBox.warning(self, "Помилка",
                                              f"Помилка при записі в файл: {str(e)}")

    def show_widget_4(self):
        self.widget_3.setVisible(False)
        self.widget_4.setVisible(True)

    def show_widget_5(self):
        self.widget_4.setVisible(False)
        self.widget_5.setVisible(True)

    def update_info_label(self):
        if self.robot:
            info_text = str(self.robot)
            self.info_label.setText(info_text)

    def selected_options(self):
        self.gender = "ж" if self.w5_radioButton_f.isChecked() else "ч"
        self.height = self.w5_comboBox_height.currentText()
        self.strength = self.w5_comboBox_strength.currentText()
        self.carrying_capacity = self.w5_comboBox_carrying_capacity.currentText()
        self.skin_color = self.w5_comboBox_skin_color.currentText()
        self.eye_color = self.w5_comboBox_eye_color.currentText()
        self.hair_length = self.w5_comboBox_hair_length.currentText()
        self.hair_color = self.w5_comboBox_hair_color.currentText()
        self.series = self.w5_comboBox_series.currentText()
        self.memory = self.w5_comboBox_memory.currentText()
        self.battery = self.w5_comboBox_battery.currentText()
        self.plus_features = []
        if self.w5_checkBox_plus_features_1.isChecked():
            self.plus_features.append("Захист від вологи")
        if self.w5_checkBox_plus_features_2.isChecked():
            self.plus_features.append("NFC-чіп")

    def show_widget_6(self):
        reply = QtWidgets.QMessageBox.question(self, 'Впевнені у своєму виборі?',
                                               "Ви впевнені, що хочете зберегти ці параметри?",
                                               QtWidgets.QMessageBox.Yes, QtWidgets.QMessageBox.No)
        if reply == QtWidgets.QMessageBox.Yes:
            self.widget_5.setVisible(False)
            self.widget_6.setVisible(True)
            self.selected_options()

            self.robot = Robot(self.gender, self.height, self.strength, self.carrying_capacity, self.skin_color,
                               self.eye_color, self.hair_length, self.hair_color, self.series, self.memory,
                               self.battery, self.plus_features)

            self.update_photo()
            self.update_info_label()
            data = [self.gender, self.height.split()[0], self.strength, self.carrying_capacity.split()[0], self.skin_color,
                    self.eye_color, self.hair_color, self.hair_length, self.series, self.memory.split()[0],
                    self.battery.split()[0], ', '.join(self.plus_features), str(self.total_price)]
            try:
                with open('robot_data.csv', 'a', newline='', encoding='utf-8') as file:
                    writer = csv.writer(file)
                    if file.tell() == 0:
                        writer.writerow(["Стать робота", "Зріст робота", "Міцність робота", "Вантажопідйомність робота",
                                         "Колір шкіри робота", "Колір очей робота", "Колір волосся робота",
                                         "Довжина волосся робота", "Серія робота", "Пам'ять робота",
                                         "Ємність акумулятора робота", "Додаткові можливості робота",
                                         "Вартість робота"])
                    writer.writerow(data)
            except Exception as e:
                QtWidgets.QMessageBox.warning(self, "Помилка",
                                              f"Помилка при записі в файл: {str(e)}")

    def update_price(self):
        self.selected_options()

        self.total_price = PriceCalculator.calculate_price(self, self.gender, self.height, self.strength,
                                                           self.carrying_capacity, self.skin_color, self.eye_color,
                                                           self.hair_length, self.hair_color, self.series, self.memory,
                                                           self.battery, self.plus_features)

        self.w5_label_price_number.setText(str(self.total_price))

