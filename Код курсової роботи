from PyQt5 import QtWidgets, QtCore, QtGui
from PIL import Image, ImageEnhance
import re
import csv

class Robot():
    def __init__(self, gender, height, strength, carrying_capacity, skin_color, eye_color,
                 hair_length, hair_color, series, memory, battery, plus_features):
        self.gender = gender
        self.height = height
        self.strength = strength
        self.carrying_capacity = carrying_capacity
        self.skin_color = skin_color
        self.eye_color = eye_color
        self.hair_length = hair_length
        self.hair_color = hair_color
        self.series = series
        self.memory = memory
        self.battery = battery
        self.plus_features = plus_features

    def __str__(self):
        return f"Пол: {self.gender}\nЗріст: {self.height}\n" \
                        f"Міцність: {self.strength}\nВантажопідйомність: {self.carrying_capacity}\n" \
                        f"Колір шкіри: {self.skin_color}\nКолір очей: {self.eye_color}\n" \
                        f"Колір волосся: {self.hair_color}\nДовжина волосся: {self.hair_length}\n" \
                        f"Серія: {self.series}\nПам'ять: {self.memory}\nЄмність акумулятора:\n{self.battery}\n" \
                        f"Додаткові можливості:\n{', '.join(self.plus_features)}"

class PriceCalculator:
    def calculate_price(self, gender, height, strength, carrying_capacity, skin_color, eye_color,
                        hair_length, hair_color, seria, memory, battery, plus_features):
        base_price = 500000

        gender_price_map = {"ж": 0, "ч": 0}
        height_price_map = {"100 см": 0, "120 см": 0, "140 см": 0, "160 см": 0, "180 см": 0, "200 см": 0, "220 см": 0}
        strength_price_map = {"Звичайна": 0, "Збільшена": 4000, "Бронезахист": 30000}
        carrying_capacity_price_map = {"100 кг": 0, "300 кг": 6000, "500 кг": 9000, "1000 кг": 15000}
        skin_color_price_map = {"Світла": 0, "Темна": 0}
        eye_color_price_map = {"Голубий": 0, "Зелений": 0, "Карий": 0}
        hair_length_price_map = {"Коротке": 0, "Довге": 0}
        hair_color_price_map = {"Чорний": 0, "Коричневий": 0, "Рудий": 0, "Блонд": 0}
        seria_price_map = {"Бюджетна": 0, "Середня": 10000, "Топ": 30000}
        memory_price_map = {"1 Т": 0, "2 Т": 20000, "3 Т": 30000, "4 Т": 40000, "5 Т": 50000}
        battery_price_map = {"10000 мАгод": 0, "15000 мАгод": 4000, "20000 мАгод": 9000, "25000 мАгод": 15000,
                             "30000 мАгод": 20000}

        gender_price = gender_price_map[gender] if gender in gender_price_map else 0
        height_price = height_price_map[height] if height in height_price_map else 0
        strength_price = strength_price_map[strength] if strength in strength_price_map else 0
        carrying_capacity_price = carrying_capacity_price_map[
            carrying_capacity] if carrying_capacity in carrying_capacity_price_map else 0
        skin_color_price = skin_color_price_map[skin_color] if skin_color in skin_color_price_map else 0
        eye_color_price = eye_color_price_map[eye_color] if eye_color in eye_color_price_map else 0
        hair_color_price = hair_color_price_map[hair_color] if hair_color in hair_color_price_map else 0
        hair_length_price = hair_length_price_map[hair_length] if hair_length in hair_length_price_map else 0
        seria_price = seria_price_map[seria] if seria in seria_price_map else 0
        memory_price = memory_price_map[memory] if memory in memory_price_map else 0
        battery_price = battery_price_map[battery] if battery in battery_price_map else 0
        plus_features_price = 0
        if "Захист від вологи" in plus_features:
            plus_features_price += 5000
        if "NFC-чіп" in plus_features:
            plus_features_price += 2000

        total_price = base_price + gender_price + height_price + strength_price + carrying_capacity_price + \
                      skin_color_price + eye_color_price + hair_length_price + hair_color_price + seria_price + \
                      memory_price + battery_price + plus_features_price
        return total_price

class MainWindow(QtWidgets.QMainWindow):
    def __init__(self):
        super().__init__()
        self.setWindowTitle("Your BFF")
        self.setFixedSize(1300, 975)
        self.move(350, 50)
        self.setWindowIcon(QtGui.QIcon('icon.png'))
        self.robot = None

        self.setup_widgets()

