    def skip_poll(self):
        reply = QtWidgets.QMessageBox.question(self, 'Підтвердження',
                                               "Ви впевнені, що хочете пропустити опитування?",
                                               QtWidgets.QMessageBox.Yes, QtWidgets.QMessageBox.No)
        if reply == QtWidgets.QMessageBox.Yes:
            data = ['', '', '', '', '', '']
            self.widget_7.setVisible(False)
            self.widget_9.setVisible(True)
            try:
                with open('user_data_2.csv', 'a', newline='', encoding='utf-8') as file:
                    writer = csv.writer(file)
                    if file.tell() == 0:
                        writer.writerow(['Стать', 'Вік', 'Зріст', 'Вибір', 'Причина'])
                    writer.writerow(data)
            except Exception as e:
                QtWidgets.QMessageBox.warning(self, "Помилка",
                                              f"Помилка при записі в файл: {str(e)}")

    def show_widget_7(self):
        self.widget_6.setVisible(False)
        self.widget_7.setVisible(True)

    def show_widget_8(self):
        self.widget_7.setVisible(False)
        self.widget_8.setVisible(True)

    def show_widget_9(self):
        if self.w8_radioButton_f.isChecked():
            self.gender_client = "ж"
        if self.w8_radioButton_m.isChecked():
            self.gender_client = "ч"
        self.age_client = str(self.w8_horizontalSlider_age.value())
        self.height_client = str(self.w8_horizontalSlider_height.value())
        self.choice_client = self.w8_comboBox_choice.currentText()
        self.reason_client = self.w8_comboBox_reason.currentText()
        data = [self.gender_client, self.age_client, self.height_client, self.choice_client, self.reason_client]
        try:
            with open('user_data_2.csv', 'a', newline='', encoding='utf-8') as file:
                writer = csv.writer(file)
                if file.tell() == 0:
                    writer.writerow(['Стать', 'Вік', 'Зріст', 'Вибір', 'Причина'])
                writer.writerow(data)
        except Exception as e:
            QtWidgets.QMessageBox.warning(self, "Помилка",
                                          f"Помилка при записі в файл: {str(e)}")

        self.widget_8.setVisible(False)
        self.widget_9.setVisible(True)

    def show_widget_1(self):
        self.widget_9.setVisible(False)
        self.widget_1.setVisible(True)

    def exit(self):
        exit()

if __name__ == "__main__":
    import sys
    app = QtWidgets.QApplication(sys.argv)
    window = MainWindow()
    window.show()
    sys.exit(app.exec_())
